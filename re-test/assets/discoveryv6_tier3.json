         {
            "contentsCategoryText":"영화",
            "contentsCategory":"movies",
            "contents":{
               "vodList":[
                  {
                     "conts_type":"MV",
                     "conts_duration":"129",
                     "item_img":"http:\/\/qt2-ngfts.lge.com\/fts\/gftsDownload.lge?biz_code=MAS&func_code=META_THUMBNAIL&file_path=\/mas\/meta_thumbnail\/poster_2013_0311_15_41_11__513d7c87a2be4[s282,414].jpg",
